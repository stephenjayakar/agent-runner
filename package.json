{
  "name": "agent-runner",
  "version": "0.1.0",
  "description": "Open-source multi-agent orchestrator for long-running autonomous coding tasks. Inspired by Cursor's scaling agents approach.",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "tsx watch src/server.ts",
    "dev:frontend": "vite",
    "build": "tsc && vite build",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.42",
    "@ai-sdk/google": "^3.0.26",
    "ai": "^6.0.82",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "nanoid": "^5.0.9",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  }
}
